{
  "name": "Kuna",
  "version": "0.0.3",
  "slug": "kuna",
  "description": "HASSIO addon to pull status information from Kuna cameras",
  "url": "https://github.com/HITChris/hassio-addons/tree/master/Kuna",
  "startup": "application",
  "boot": "auto",
  "host_network": false,
  "hassio_api": false,
  "homeassistant_api":true,
  "options": {
    "kuna_email": "MyKunaEmailAddress",
    "kuna_password": "MyKunaPassword",
    "refresh_seconds": 15,
    "save_thumbnail": "false"
  },
  "schema": {
    "kuna_email": "str",
    "kuna_password": "str",
    "refresh_seconds": "int",
    "save_thumbnail": "str"
  }
}